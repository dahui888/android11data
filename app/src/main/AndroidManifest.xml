<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.itloser.utils.android11data">

    <queries>
        <!-- 声明所要访问的应用 -->
        <package android:name="com.google.android.documentsui" />

    </queries>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_documents"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_documents"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android11data">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>